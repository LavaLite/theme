        "laravel-mix": "^2.0",
        "lodash": "^4.17.5",
        "popper.js": "^1.12",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.4.1",
        "eonasdan-bootstrap-datetimepicker": "^4.17",
        "codemirror": "^5.33",
        "dropzone": "^5.3",
        "fullcalendar": "^3.8",
        "jquery-validation": "^1.17",
        "summernote": "^0.8",
        "sweetalert": "^2.1.0",
        "timeago": "^1.6",
        "toastr": "^2.1.4"
    }
